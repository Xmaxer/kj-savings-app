		"tailwind-variants": "1.0.0",
		"tailwindcss": "3.4.17",
		"tailwindcss-animate": "1.0.7",
		"typescript": "5.8.3",
		"typescript-eslint": "8.34.0",
		"vite": "6.3.5"
	},
	"dependencies": {
		"@tailwindcss/postcss": "4.1.9",
		"@types/node": "24.0.0",
		"chart.js": "4.4.9",
		"chartjs-plugin-annotation": "3.1.0"
	}
